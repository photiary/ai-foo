{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/schema/food-analysis-image-only-schema.json",
  "title": "FoodAnalysisResponseImageOnly",
  "type": "object",
  "properties": {
    "foods": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "position": {
            "type": "object",
            "properties": {
              "x": { "type": "integer" },
              "y": { "type": "integer" }
            },
            "required": ["x", "y"],
            "additionalProperties": false
          }
        },
        "required": ["name", "position"],
        "additionalProperties": false
      }
    }
  },
  "required": ["foods"],
  "additionalProperties": false
}
